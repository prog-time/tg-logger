{
    "name": "prog-time/tg-logger",
    "description": "A Laravel package for sending logs to Telegram",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "source": "https://github.com/prog-time/tg-logger"
    },
    "authors": [
        {
            "name": "Prog-Time",
            "email": "iliya-rabota97@mail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ProgTime\\TgLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProgTime\\TelegramLogger\\TelegramLoggerServiceProvider"
            ]
        }
    }
}
